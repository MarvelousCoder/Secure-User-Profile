generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  name        String
  email       String   @unique
  password    String
  encryptedId String // AES-encrypted government ID
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
